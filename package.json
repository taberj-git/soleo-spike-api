{
  "name": "soleo-spike-api",
  "version": "1.0.0",
  "description": "Node-based web API using Express and TypeScript",
  "type": "module",
  "main": "./dist/server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taberj-git/soleo-spike-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/taberj-git/soleo-spike-api/issues"
  },
  "homepage": "https://github.com/taberj-git/soleo-spike-api#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "npm run clean && tsc",
    "clean": "rm -rf dist",
    "test": "NODE_OPTIONS='--experimental-vm-modules --localstorage-file=/tmp/jest-ls' jest",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules --localstorage-file=/tmp/jest-ls' jest --watch",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules --localstorage-file=/tmp/jest-ls' jest --coverage",
    "test:ci": "NODE_OPTIONS='--experimental-vm-modules --localstorage-file=/tmp/jest-ls' jest --ci --coverage --maxWorkers=2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.940.0",
    "@azure/storage-blob": "^12.29.1",
    "@google-cloud/storage": "^7.17.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.0",
    "express-rate-limit": "^8.2.1",
    "flatted": "^3.3.3",
    "helmet": "^8.1.0",
    "multer": "^2.0.2",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "@types/winston": "^2.4.4",
    "jest": "^30.2.0",
    "jest-environment-node-single-context": "^29.4.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
